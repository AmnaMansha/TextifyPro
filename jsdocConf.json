{
  "plugins": [
    "plugins/markdown"
  ],
  "recurseDepth": 10,
  "opts": {
    "template": "node_modules/docdash",
    "destination": "./docs/",
    "recurse": true,
    "verbose": true
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.jsx?$",
    "excludePattern": "(node_modules|dist|build)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "sort": true,
    "search": true,
    "collapse": false
  },
  "typescript": {
    "moduleRoot": "src"
  }
}